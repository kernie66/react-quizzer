{
    "root": true,
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "jest/unbound-method": "off",
      "jest/require-hook": "off"
    }
}
